{
  "$schema": "..\\node_modules/@tauri-apps/cli\\schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../dist",
    "distDir": "../dist",
	"withGlobalTauri": true
  },
  "package": {
    "productName": "datapack_updater",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
		"fs": {
			"all": true,
			"readFile": true,
			"writeFile": true
		},
		"dialog": {
			"all": true,
			"open": true,
			"save": true
		}
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.ico"
      ],
      "identifier": "com.cmd-golem.datapack_updater",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 740,
        "resizable": true,
        "title": "Pack Updater",
        "width": 1270
      }
    ]
  }
}